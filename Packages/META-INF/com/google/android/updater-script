
ui_print("   ...TEAM_Gummy SGS4 Kernel...");
ui_print("");
ui_print("{x}...Mounting system");
run_program("/sbin/busybox", "mount", "/system");
ui_print("{x}...Copying modules");
ui_print("");
package_extract_dir("system", "/system");
ui_print("{x}...Unmounting system");
ui_print("");
unmount("/system");
ui_print("{x}...Flashing kernel");
ui_print("");

package_extract_file("boot.lok", "/dev/block/platform/msm_sdcc.1/by-name/boot");

show_progress(0.100000, 0);
